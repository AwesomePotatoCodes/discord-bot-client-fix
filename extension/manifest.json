{
	"manifest_version": 3,
	"name": "Discord Bot Client",
	"version": "1.0.0",
	"description": "This extension allows you to use https://botclient.tk",
	"author": "Flam3rboy, Fixed by AwesomePotatoCodes",
	"background": {
		"scripts": ["script.js"]
	},
	
	"permissions": ["webRequest", "webRequestBlocking"],
	"host_permissions": ["<all_urls>"],
	
	"page_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"96": "images/96.png",
		"128": "images/128.png",
		"256": "images/256.png"
	}
}
